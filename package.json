{
  "name": "@beqom/alto-ui",
  "version": "0.6.4",
  "main": "index.js",
  "repository": "https://github.com/beqom/alto",
  "author": "Renaud Tertrais <renaud.tertrais@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test:output": "jest --json --outputFile=.jest-test-results.json || true",
    "test": "jest --coverage",
    "watch:test": "jest --watch",
    "lint:style": "stylelint \"src/*.scss\" \"src/**/*.scss\" \"src/*.css\" \"src/**/*.css\"",
    "lint:script": "eslint .",
    "lint": "npm run lint:script && npm run lint:style",
    "clean:build": "rimraf build",
    "clean:dist": "rimraf dist",
    "build:storybook": "cross-env NODE_ENV=development build-storybook -c .storybook -o build/storybook",
    "start:storybook": "start-storybook -p 9006 -c .storybook",
    "prebuild:alto-ui": "npm run lint && npm run test && npm run clean:dist && copy ./package.json ./README.md ./dist",
    "build:alto-ui": "cross-env NODE_ENV=production babel ./src/alto-ui -d ./dist --copy-files --ignore story.js,__tests__,README.md",
    "watch:alto-ui": "cross-env NODE_ENV=production babel ./src/alto-ui -d ./dist -w --copy-files --ignore story.js,__tests__,README.md",
    "sass:app": "node-sass ./src/app/scss/site.scss ./src/app/static/css/site.css",
    "hugo:app": "hugo --source ./src/app -d ../../build",
    "serve:app": "browser-sync start --config ./bs-config.js",
    "build:app": "npm run sass:app && npm run hugo:app",
    "start:app": "npm run sass:app -- -w | npm run hugo:app -- --watch | npm run serve:app",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "prebuild": "npm run clean:build && npm run lint && npm run test",
    "build": "npm run build:app && npm run build:storybook",
    "start": "npm run start:storybook"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^3.2.16",
    "@storybook/addon-centered": "3.2.16",
    "@storybook/addon-info": "3.2.16",
    "@storybook/addon-jest": "^3.2.16",
    "@storybook/addon-knobs": "3.2.16",
    "@storybook/addon-options": "3.2.16",
    "@storybook/react": "3.2.16",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.18.13",
    "clarity-icons": "^0.10.17",
    "copy": "^0.3.1",
    "cross-env": "^5.1.1",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "gh-pages": "^1.0.0",
    "github-markdown-css": "^2.9.0",
    "hugo-cli": "^0.5.4",
    "jest-cli": "^21.2.1",
    "jquery": "^3.2.1",
    "node-sass": "^4.5.3",
    "prop-types": "^15.6.0",
    "raw-loader": "^0.5.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "storybook-addon-jsx": "^4.2.0",
    "storybook-readme": "^3.0.6",
    "stylelint": "^8.2.0",
    "stylelint-config-recommended": "^1.0.0",
    "stylelint-scss": "^2.1.0",
    "webpack": "^3.10.0",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "@webcomponents/custom-elements": "^1.0.0",
    "classnames": "^2.2.5",
    "date-fns": "^1.29.0",
    "lodash.get": "^4.4.2",
    "lodash.isarray": "^4.0.0",
    "lodash.omit": "^4.5.0",
    "lodash.range": "^3.2.0",
    "styled-components": "^2.2.3"
  },
  "peerDependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "browserslist": [
    "last 3 versions",
    "IE >= 11"
  ],
  "jest": {
    "verbose": true,
    "coverageDirectory": "coverage/",
    "notify": true,
    "setupFiles": [
      "<rootDir>/.jest-shim.js",
      "<rootDir>/.jest-setup.js"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.s?css$": "<rootDir>/__mocks__/styleMock.js"
    },
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "src/*.{js,jsx}",
      "!src/.*",
      "!src/__test__/*",
      "!src/**/__test__/*",
      "!src/**/index.js",
      "!src/**/story.js"
    ]
  }
}
